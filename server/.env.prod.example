# Environment Mode
NODE_ENV=production

# Production Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=react-nest-template
DB_USERNAME=react-nest-template_user
DB_PASSWORD=CHANGE-THIS-TO-SECURE-PASSWORD

# Production JWT Configuration
JWT_SECRET=CHANGE-THIS-TO-VERY-SECURE-JWT-SECRET-AT-LEAST-64-CHARACTERS-LONG

# Production Frontend URLs
FRONTEND_URL=https://google.com
CORS_ORIGIN=https://google.com

# Production Logging
LOG_LEVEL=warn

# Production Metrics
ENABLE_METRICS=true

# Production Features
BACKUP_ENABLED=true
MAINTENANCE_MODE=false